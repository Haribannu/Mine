import java.util.*;
class Solution {
    public static void main(String args[])
 {
        Scanner sc=new Scanner(System.in);
	String str[]={"h","e","l","l","o"};
        int i,n=str.length;
        int start=0,end=n-1;
        for(i=end;i>=0;i--)
	{
	System.out.print(str[i]);

        
        start++;
        end--;
        
        }

    }
}
